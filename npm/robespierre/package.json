{
  "name": "robespierre",
  "version": "1.0.0-beta.2",
  "description": "Find extraneous and implicit dependencies by comparing your manifest and actual imports in your code using AST parsing",
  "bin": "lib/index.js",
  "scripts": {
    "typecheck": "npx tsc --noEmit",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "build": "npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarioArnt/robespierre.git"
  },
  "keywords": [
    "npm",
    "extraneous",
    "implicit",
    "code-quality",
    "rust"
  ],
  "author": "Mario Arnautou <mario.arnautou@gmail.com>",
  "contributors": ["Thibault Ruby <thibault.ruby.pro@gmail.com>", "Mario Arnautou <mario.arnautou@gmail.com>"],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/MarioArnt/robespierre/issues"
  },
  "homepage": "https://github.com/MarioArnt/robespierre#readme",
  "devDependencies": {
    "@types/node": "^20.11.4",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "robespierre-linux-x64": "1.0.0-beta.2",
    "robespierre-linux-arm64": "1.0.0-beta.2",
    "robespierre-darwin-x64": "1.0.0-beta.2",
    "robespierre-darwin-arm64": "1.0.0-beta.2",
    "robespierre-windows-x64": "1.0.0-beta.2",
    "robespierre-windows-arm64": "1.0.0-beta.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "ignorePatterns": [
      "lib/*"
    ],
    "root": true
  }
}
